<?xml version="1.0" encoding="UTF-8"?>
<CHECKLIST_SET>
<CHECKLIST accession="COPO_FAANG" checklistType="Sample">
    <IDENTIFIERS>
        <PRIMARY_ID>COPO_FAANG</PRIMARY_ID>
        <ENA_CHECKLIST_ID>ERC000011</ENA_CHECKLIST_ID>
    </IDENTIFIERS>
    <DESCRIPTOR>
        <LABEL>FAANG sample checklist</LABEL>
        <NAME>FAANG sample checklist</NAME>
        <DESCRIPTION>Minimum information required for Functional Annotation of Animal Genomes (FAANG) samples</DESCRIPTION>
        <AUTHORITY>COPO</AUTHORITY>
        <FIELD_GROUP restrictionType="Any number or none of the fields">
            <NAME>sample collection</NAME>
            <FIELD>
                <LABEL>Sample</LABEL>
                <NAME>sample</NAME>
                <DESCRIPTION>A unique alphanumeric reference or identifier for the sample. Example: sample123</DESCRIPTION>
                 <FIELD_TYPE>
                    <TEXT_FIELD>
                        <REGEX_VALUE>^[a-zA-Z0-9]+$</REGEX_VALUE>
                    </TEXT_FIELD>
                </FIELD_TYPE>
                <MANDATORY>mandatory</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
            <FIELD>
                <LABEL>Organism</LABEL>
                <NAME>organism</NAME>
                <DESCRIPTION>NCBI taxon ID of organism. Example: 9606</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_FIELD>
                        <TAXON_ID_FIELD/>
                    </TEXT_FIELD>
                </FIELD_TYPE>
                <MANDATORY>mandatory</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
            <FIELD>
                <LABEL>Health Status</LABEL>
                <NAME>health_status</NAME>
                <DESCRIPTION>Healthy animals should have the term normal, otherwise use the as many disease terms as necessary from EFO. Example: normal</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_FIELD/>
                </FIELD_TYPE>
                <MANDATORY>optional</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
            <FIELD>
                <LABEL>Sex</LABEL>
                <SYNONYM>sex</SYNONYM>
                <NAME>sex</NAME>
                <DESCRIPTION>Sex of the organism from which the sample was obtained. It can be described in any child term of PATO_0000047. Example: female</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_CHOICE_FIELD>
                        <TEXT_VALUE>
                            <VALUE>diandrous</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>female</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>female with DSD</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>hermaphrodite</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>male</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>male with DSD</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>monandrous</VALUE>
                        </TEXT_VALUE>
                        <TEXT_VALUE>
                            <VALUE>pseuodohermaphrodite</VALUE>
                        </TEXT_VALUE>
                    </TEXT_CHOICE_FIELD>
                </FIELD_TYPE>
                <MANDATORY>mandatory</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
             <FIELD>
                <LABEL>Birth Date</LABEL>
                <NAME>birth_date</NAME>
                <DESCRIPTION>Birth date, in the format YYYY-MM-DD, or YYYY-MM where only the month is known. For embryo samples record 'not applicable'. Example: 2025-01-24</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_FIELD>
                        <REGEX_VALUE>^\d{4}-(0[1-9]|1[0-2])(-([0-2]\d|3[01]))?$</REGEX_VALUE>
                    </TEXT_FIELD>
                </FIELD_TYPE>
                <MANDATORY>optional</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
             <FIELD>
                <LABEL>Breed</LABEL>
                <NAME>breed</NAME>
                <DESCRIPTION>Animal breed, described using the FAANG breed description guidelines (http://bit.ly/FAANGbreed). Should be considered mandatory for terrestiral species, for aquatic species record 'not applicable'. Example: Large Black</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_FIELD/>
                </FIELD_TYPE>
                <MANDATORY>optional</MANDATORY>
                <MULTIPLICITY>single</MULTIPLICITY>
            </FIELD>
            <FIELD>
                <LABEL>Specimen Collection Date</LABEL>
                <SYNONYM>collection date</SYNONYM>
                <NAME>specimen_collection_date</NAME>
                <DESCRIPTION>The date the sample was collected either as an instance (single point in time) or interval. In case no exact time is available, the date/time can be right truncated i.e. all of these are valid ISO8601 compliant times: 2008-01-23T19:23:10+00:00; 2008-01-23T19:23:10; 2008-01-23; 2008-01; 2008. Example: 2024-01-19</DESCRIPTION>
                <FIELD_TYPE>
                    <TEXT_FIELD>
                        <REGEX_VALUE>(^[12][0-9]{3}(-(0[1-9]|1[0-2])(-(0[1-9]|[12][0-9]|3[01])(T[0-9]{2}:[0-9]{2}(:[0-9]{2})?Z?([+-][0-9]{1,2})?)?)?)?(/[0-9]{4}(-[0-9]{2}(-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2})?Z?([+-][0-9]{1,2})?)?)?)?)?$)|(^not applicable$)|(^not collected$)|(^not provided$)|(^restricted access$)|(^missing: control sample$)|(^missing: sample group$)|(^missing: synthetic construct$)|(^missing: lab stock$)|(^missing: third party data$)|(^missing: data agreement established pre-2023$)|(^missing: endangered species$)|(^missing: human-identifiable$)|(^missing$)</REGEX_VALUE>
                    </TEXT_FIELD>
                </FIELD_TYPE>
                <MANDATORY>mandatory</MANDATORY>
                <MULTIPLICITY>multiple</MULTIPLICITY>
             </FIELD>
             <FIELD>
