                <VALUE>Air dried</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Dry ice</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Ethanol/Dry ice slurry</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Lyophilised</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Not Applicable</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Not Collected</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Not Provided</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Other</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>RNALater</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Snap frozen</VALUE>
              </TEXT_VALUE>
